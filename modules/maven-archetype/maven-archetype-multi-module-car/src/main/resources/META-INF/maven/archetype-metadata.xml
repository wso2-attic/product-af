<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor xsi:schemaLocation="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0 http://maven.apache.org/xsd/archetype-descriptor-1.0.0.xsd" name="SampleCARMavenProject"
    xmlns="http://maven.apache.org/plugins/maven-archetype-plugin/archetype-descriptor/1.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modules>
    <module id="built_artifact" dir="built_artifact" name="built_artifact">
      <fileSets>
    	<fileSet filtered="true" encoding="UTF-8">
          <directory/>
          <includes>
            <include>*.xml</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory/>
          <includes>
            <include>**/*.xml</include>
          </includes>
        </fileSet>    
        <fileSet filtered="true" encoding="UTF-8">
          <directory/>
          <includes>
            <include>**/*.wsdl</include>
          </includes>
        </fileSet>    
      </fileSets>
    </module>
    <module id="ApplicationResources" dir="ApplicationResources" name="ApplicationResources">
      <fileSets>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>Production</directory>
          <includes>
            <include>**/*.xml</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>Testing</directory>
          <includes>
            <include>**/*.xml</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>Development</directory>
          <includes>
            <include>**/*.xml</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory></directory>
          <includes>
            <include>artifact.xml</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory></directory>
          <includes>
            <include>echo.wsdl</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="SimpleProxy" dir="SimpleProxy" name="SimpleProxy">
      <fileSets>
        <fileSet filtered="true" encoding="UTF-8">
          <directory>src/main/synapse-config</directory>
          <includes>
            <include>**/*.xml</include>
          </includes>
        </fileSet>
        <fileSet filtered="true" encoding="UTF-8">
          <directory></directory>
          <includes>
            <include>artifact.xml</include>
          </includes>
        </fileSet>
      </fileSets>
    </module>
    <module id="__rootArtifactId__DevelopmentResourcesCAR" dir="__rootArtifactId__DevelopmentResourcesCAR" name="__rootArtifactId__DevelopmentResourcesCAR" />
    <module id="__rootArtifactId__TestingResourcesCAR" dir="__rootArtifactId__TestingResourcesCAR" name="__rootArtifactId__TestingResourcesCAR" />
    <module id="__rootArtifactId__" dir="__rootArtifactId__" name="__rootArtifactId__" />
    <module id="__rootArtifactId__ProductionResourcesCAR" dir="__rootArtifactId__ProductionResourcesCAR" name="__rootArtifactId__ProductionResourcesCAR" />
  </modules>
</archetype-descriptor>
