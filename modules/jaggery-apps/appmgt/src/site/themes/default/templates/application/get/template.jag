           }, function (result) {
               jagg.message({
                   content: "The Deployment is underway. Please wait and refresh page after few minutes.",
                   type: 'success',
                   id:'notification'
               });
               if((stage == fgdmAllowedStage) && hasDomainMappingPermission === 'true' &&   //has permissions
